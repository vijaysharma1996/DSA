//Find the maximum and minimum element in an array.

#include<iostream>
using namespace std;
int main()
{
	int arr[4]={1,2,3,-4};
	int max=arr[0];
	int min=arr[0];
	for(int i=0;i<4;i++)
        { 
            if(arr[i]>max)
                max=arr[i];
            if(arr[i]<min)
                min=arr[i];
            
        } 
	cout<<"max-"<<max<<endl;
	cout<<"min-"<<min;
    
return 0;
}
